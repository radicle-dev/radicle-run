ainer margin-vert--xl\">\n        <div className=\"row\">\n          <div className=\"col col--8 col--offset-2\">\n            <h1>Tags</h1>\n            <div className=\"margin-vert--lg\">{tagsSection}</div>\n          </div>\n        </div>\n      </div>\n    </Layout>\n  );\n}\n\nexport default BlogTagsListPage;\n"]}]}